class Solution {
    public void moveZeroes(int[] nums) {
        int i=0,j,p;
        int l = nums.length;
        
        for(j=0;j<l;j++)
        { 
            if(nums[j]!=0)
            {
                nums[i]=nums[j];
                i++;
            }
        }
        for(j=i;j<l;j++)
        {
            nums[j]=0;
        }
    }
}
